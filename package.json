{
  "name": "wikidata-hub",
  "version": "1.0.0",
  "description": "a small webservice to handle redirections from a Wikidata id to Wikimedia sites and beyond",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "watch": "supervisor -w index.js,lib,config index.js",
    "postinstall": "./scripts/postinstall",
    "lint": "standard",
    "update-toc": "doctoc README.md",
    "update-properties": "./scripts/update_properties",
    "test": "mocha"
  },
  "keywords": [
    "wikidata",
    "wikipedia",
    "redirections",
    "images"
  ],
  "author": "maxlath",
  "license": "MIT",
  "dependencies": {
    "bluereq": "^2.1.4",
    "chalk": "^2.3.0",
    "config": "^1.28.1",
    "express": "^4.16.2",
    "inv-loggers": "^3.3.1",
    "morgan": "^1.9.0",
    "wikidata-cli": "^5.6.1",
    "wikidata-sdk": "^5.6.1"
  },
  "devDependencies": {
    "doctoc": "^1.3.0",
    "git-hooks": "^1.1.9",
    "mocha": "^4.0.1",
    "should": "^13.1.3",
    "standard": "^10.0.3",
    "supervisor": "^0.12.0"
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "globals": [
      "it",
      "describe"
    ]
  }
}
